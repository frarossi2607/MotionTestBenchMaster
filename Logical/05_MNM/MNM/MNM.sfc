(* @SFCCOMMENT := '' *)
ENTRY_ACTION
(* @LANGUAGE := 'st' *)
VAR_MNM_AutoCombiStep := MNM_AutoCombiInit;//V1.3
END_ACTION
TRANSITION IdleMode FROM MNMAutoCombi_BAF TO MNMIdle:=
(* @LANGUAGE := 'st' *)
NOT (OPS.DataOut.BlowerTrasferGateOpened AND OPS.DataOut.ActisTrasferGateOpened) OR (NOT OPS.DataOut.FlrModeSelectorAuto AND NOT MC_InputOutput.Network.Machine.Status.MachineRunning) OR//V4.A.A.1.7.0
NOT ((CFG_Par.Machine.Combi.BLF AND HMI_CmdBar_PB[PB_FillerLabellerCoupling].Command) OR 
(CFG_Par.Machine.Combi.BF AND HMI_CmdBar_PB[PB_FillerBlowerCoupling].Command) OR 
(CFG_Par.Machine.Combi.BAF 	AND HMI_CmdBar_PB[PB_FillerActisCoupled].Command)) OR //V4.A.A.1.7.0
TMR_TON_TimeoutModality.Q
END_TRANSITION
(* @SFCISJUMP := 'MNMIdle' *)
STEP MNMAutoCombi:
(* @LANGUAGE := 'st' *)
MC_InputOutput.Network.Machine.Commands.AutoCombiModalityRequest	:= TRUE;

IF (VAR_MNM_NormalStop OR VAR_MNM_QuickStop OR VAR_MNM_NormalStopWithJog OR VAR_MNM_QuickStopWithJog OR VAR_MNM_StopAfterEmpty OR VAR_MNM_StopIfNotEmpty) AND (VAR_MNM_AutoCombiStep > MNM_AutoCombiBLFWaitStart) THEN//V1.3
	VAR_MNM_AutoCombiStep := MNM_AutoCombiFault;
ELSIF VAR_MNM_DisengageForStop THEN
	VAR_MNM_AutoCombiStep := MNM_AutoCombiMachineFixEmptying;
ELSIF NOT (NOT MC_InputOutput.Network.Machine.Status.NoModalityActive AND MC_InputOutput.Network.Machine.Status.AutoCombiModalityActive) THEN
	VAR_MNM_AutoCombiStep := MNM_AutoCombiInit;//V1.3
END_IF

TMR_TON_StartingHornElapsed.IN := FALSE;
MC_InputOutput.Network.Machine.Commands.MachineStart		:= FALSE;
MNM.DataOut.DesyncroRequest	:= FALSE;
VAR_MNM_HornFlrRequest	:= FALSE;
MNM.DataOut.StartLightFixRequest	:= FALSE;
MNM.DataOut.StartLightBlinkRequest	:= FALSE;

CASE VAR_MNM_AutoCombiStep OF
	MNM_AutoCombiInit://V1.3
		IF CFG_Par.Machine.Combi.BF THEN//V1.3
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiBFWaitStart;//V1.3
		ELSIF CFG_Par.Machine.Combi.BLF THEN//V1.3
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiBLFWaitStart;//V1.3
		END_IF//V1.3
		
	MNM_AutoCombiBFWaitStart://V1.3
		IF MC_InputOutput.Network.Machine.Status.EnableStart AND
			NOT (VAR_MNM_NormalStop OR VAR_MNM_QuickStop OR VAR_MNM_NormalStopWithJog OR VAR_MNM_QuickStopWithJog OR VAR_MNM_StopAfterEmpty OR VAR_MNM_StopIfNotEmpty) THEN//V1.3
//			IF NOT BlrFlr.Stopped THEN//V1.3 //v1.6
//				IF EDGEPOS(BlrFlr.CmdStart) THEN//V1.3 //v1.6
//					VAR_MNM_AutoCombiStep	:= MNM_AutoCombiSyncroStart;//V1.3 //v1.6
//				END_IF//V1.3 //v1.6
//			ELSE//V1.3 //v1.6
				IF (BlrFlr.PbStart OR (IO_PB_FlStart.ON AND IO_PB_FlStart.Enable)) THEN//V1.3
					VAR_MNM_AutoCombiStep	:= MNM_AutoCombiHornBlow;//V1.3
				END_IF//V1.3
//			END_IF//V1.3 //v1.6
		END_IF//V1.3

		IF (BlrFlr.PbStart OR (IO_PB_FlStart.ON AND IO_PB_FlStart.Enable)) THEN//V1.3
			MNM.DataOut.StartLightFixRequest	:= TRUE;//V1.3
		ELSE//V1.3
			MNM.DataOut.StartLightBlinkRequest	:= IO_PB_FlStart.Enable;//V1.3
		END_IF//V1.3
	
	MNM_AutoCombiBLFWaitStart://V1.3
		IF MC_InputOutput.Network.Machine.Status.EnableStart AND
			NOT (VAR_MNM_NormalStop OR VAR_MNM_QuickStop OR VAR_MNM_NormalStopWithJog OR VAR_MNM_QuickStopWithJog OR VAR_MNM_StopAfterEmpty OR VAR_MNM_StopIfNotEmpty) THEN//V1.3
			IF LbrFlr.Stopped THEN//V1.3
				IF EDGEPOS(LbrFlr.CmdStart) THEN//V1.3
					VAR_MNM_AutoCombiStep	:= MNM_AutoCombiSyncroStart;//V1.3
				END_IF//V1.3
			ELSE//V1.3
				IF (BlrFlr.PbStart OR IO_PB_LbStart.ON OR (IO_PB_FlStart.ON AND IO_PB_FlStart.Enable)) THEN//V1.3
					VAR_MNM_AutoCombiStep	:= MNM_AutoCombiHornBlow;//V1.3
				END_IF//V1.3
			END_IF//V1.3
		END_IF//V1.3
		
		IF (BlrFlr.PbStart OR IO_PB_LbStart.ON OR (IO_PB_FlStart.ON AND IO_PB_FlStart.Enable)) THEN//V1.3
			MNM.DataOut.StartLightFixRequest	:= TRUE;//V1.3
		ELSE//V1.3
			MNM.DataOut.StartLightBlinkRequest	:= IO_PB_FlStart.Enable;//V1.3
		END_IF//V1.3
			
	MNM_AutoCombiHornBlow://V1.3
		MNM.DataOut.StartLightFixRequest	:= TRUE;//V1.3
		TMR_TON_StartingHornElapsed.IN		:= TRUE;//V1.3
		VAR_MNM_HornFlrRequest	:= TRUE;//V1.3
		
		IF MC_InputOutput.Network.Machine.Status.EnableStart THEN//V1.3
			IF CFG_Par.Machine.Combi.BLF AND NOT (BlrFlr.PbStart OR IO_PB_LbStart.ON OR (IO_PB_FlStart.ON AND IO_PB_FlStart.Enable)) THEN//V1.3
				VAR_MNM_AutoCombiStep := MNM_AutoCombiBLFWaitStart;//V1.3
			ELSIF CFG_Par.Machine.Combi.BF AND NOT (BlrFlr.PbStart OR (IO_PB_FlStart.ON AND IO_PB_FlStart.Enable)) THEN//V1.3
				VAR_MNM_AutoCombiStep := MNM_AutoCombiBFWaitStart;//V1.3
			END_IF//V1.3
		END_IF//V1.3
		
		IF TMR_TON_StartingHornElapsed.Q THEN//V1.3
			VAR_MNM_AutoCombiStep := MNM_AutoCombiSyncroStart;//V1.3
		END_IF//V1.3
		
	MNM_AutoCombiSyncroStart:
		IF CFG_MC.Generic.SyncroPar.ExternalEncoder THEN//V1.3
			MC_InputOutput.Network.Machine.Commands.MachineStart := NOT BlrFlr.Stopped;//V1.3
		ELSE//V1.3
			MC_InputOutput.Network.Machine.Commands.MachineStart := TRUE;//V1.3
		END_IF//V1.3
	
		IF MC_InputOutput.Network.Machine.Status.CombiSynchronized THEN
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineReadyToRun;
		END_IF
		
		MNM.DataOut.StartLightBlinkRequest	:= TRUE;
	
	MNM_AutoCombiMachineReadyToRun:
		MC_InputOutput.Network.Machine.Commands.MachineStart := TRUE;
	
		MNM.DataOut.StartLightBlinkRequest	:= TRUE;
		
		IF MC_InputOutput.Network.Machine.Status.MachineRunning THEN
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineRunningEmpty;
		END_IF
	
	MNM_AutoCombiMachineRunningEmpty:	//Wait stop PB or Fault
		MC_InputOutput.Network.Machine.Commands.MachineStart := TRUE;
	
		MNM.DataOut.StartLightFixRequest	:= MC_InputOutput.Network.Machine.Status.MachineRunning;
		MNM.DataOut.StartLightBlinkRequest	:= NOT MC_InputOutput.Network.Machine.Status.MachineRunning;

		IF CFG_Par.Machine.Combi.BLF AND LbrFlr.DesynchroReq AND LbrFlr.MachOutFree THEN
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineFixEmptying;
		ELSIF CFG_Par.Machine.Combi.BF AND NOT BlrFlr.NoAlarm THEN//v1.4
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineFixEmptying;//v1.4
		ELSIF MC_InputOutput.Network.Machine.Status.MachineRunning AND NOT SFT.DataOut.CombiEmpty THEN
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineRunningFull;
		ELSIF NOT MC_InputOutput.Network.Machine.Status.MachineRunning THEN
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineReadyToRun;
		END_IF
		
		IF CFG_Par.Machine.Combi.BF THEN//V4.A.A.1.6.6
			IF EDGENEG(BlrFlr.CmdStart) THEN//V4.A.A.1.6.6
				VAR_MNM_MachineStopRequest := TRUE ;//V4.A.A.1.6.6
			END_IF
			IF VAR_MNM_MachineStopRequest AND (MC_InputOutput.Network.Machine.Status.ActualSpeed <= 5000.0) THEN//V4.A.A.1.6.6
				VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineFixEmptying;//V4.A.A.1.6.6
			END_IF//V4.A.A.1.6.6
		END_IF//V4.A.A.1.6.6
		
		
		IF EDGEPOS(HMI_MNM_PB[PB_MNM_AxisInMotionAdjustment].ON) THEN
			MC_InputOutput.Network.Machine.Parameters.InMotionTimerValue	:= HMI_MNM_AxisInMotionAdjustment;
			MC_InputOutput.Network.Machine.Commands.InMotionTimerModReq		:= TRUE;
			ALR[686].Active	:= TRUE;
		ELSIF MC_InputOutput.Network.Machine.Commands.InMotionTimerModReq AND MC_InputOutput.Network.Machine.Status.InMotionTimerDone THEN
			MC_InputOutput.Network.Machine.Commands.InMotionTimerModReq		:= FALSE;
			HMI_MNM_AxisInMotionAdjustment	:= 0.0;
			ALR[686].Active	:= FALSE;
		ELSIF NOT MC_InputOutput.Network.Machine.Commands.InMotionTimerModReq THEN
			ALR[686].Active	:= FALSE;
		END_IF
	
	MNM_AutoCombiMachineRunningFull:	//Wait stop PB or Fault
		MC_InputOutput.Network.Machine.Commands.MachineStart := TRUE;
	
		MNM.DataOut.StartLightFixRequest	:= MC_InputOutput.Network.Machine.Status.MachineRunning;
		MNM.DataOut.StartLightBlinkRequest	:= NOT MC_InputOutput.Network.Machine.Status.MachineRunning;

		IF CFG_Par.Machine.Combi.BLF AND LbrFlr.DesynchroReq AND LbrFlr.MachOutFree THEN
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineFixEmptying;
		ELSIF CFG_Par.Machine.Combi.BF AND NOT BlrFlr.NoAlarm THEN//v1.4
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineFixEmptying;//v1.4
		ELSIF MC_InputOutput.Network.Machine.Status.MachineRunning AND SFT.DataOut.CombiEmpty THEN
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineRunningEmpty;
		ELSIF NOT MC_InputOutput.Network.Machine.Status.MachineRunning THEN
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineReadyToRun;
		END_IF
		
		IF CFG_Par.Machine.Combi.BF THEN//V4.A.A.1.6.6
			IF EDGENEG(BlrFlr.CmdStart)  THEN//V4.A.A.1.6.6
				VAR_MNM_MachineStopRequest := TRUE ;//V4.A.A.1.6.6
			END_IF
			IF VAR_MNM_MachineStopRequest AND (MC_InputOutput.Network.Machine.Status.ActualSpeed <= 5000.0) THEN//V4.A.A.1.6.6
				VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineFixEmptying;//V4.A.A.1.6.6
				END_IF//V4.A.A.1.6.6
			END_IF//V4.A.A.1.6.6
	
	MNM_AutoCombiMachineFixEmptying:
		MC_InputOutput.Network.Machine.Commands.MachineStart		:= TRUE;
		MNM.DataOut.DesyncroRequest	:= TRUE;
	
		MNM.DataOut.StartLightFixRequest	:= MC_InputOutput.Network.Machine.Status.MachineRunning;
		MNM.DataOut.StartLightBlinkRequest	:= NOT MC_InputOutput.Network.Machine.Status.MachineRunning;

		IF SFT.DataOut.MachineEmpty THEN
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiMachineStopping;
		END_IF
	
	MNM_AutoCombiMachineStopping:
		IF NOT MC_InputOutput.Network.Machine.Status.MachineRunning THEN
			VAR_MNM_MachineStopRequest := FALSE;//V4.A.A.1.6.6
			VAR_MNM_AutoCombiStep	:= MNM_AutoCombiInit;//V1.3
		END_IF
	
	MNM_AutoCombiFault:
		MC_InputOutput.Network.Machine.Commands.MachineStart := NOT CFG_MC.Generic.DisengageImmediately;
			
		MNM.DataOut.StartLightFixRequest	:= MC_InputOutput.Network.Machine.Status.MachineRunning;
		MNM.DataOut.StartLightBlinkRequest	:= NOT MC_InputOutput.Network.Machine.Status.MachineRunning;

		IF NOT (VAR_MNM_NormalStop OR VAR_MNM_QuickStop OR VAR_MNM_NormalStopWithJog OR VAR_MNM_QuickStopWithJog OR VAR_MNM_StopAfterEmpty) AND NOT MC_InputOutput.Network.Machine.Status.MachineRunning THEN//V1.61//v1.65
			VAR_MNM_AutoCombiStep := MNM_AutoCombiInit;//V1.3
		END_IF
END_CASE

TMR_TON_StartingHornElapsed(PT:= t#2s);
END_STEP
